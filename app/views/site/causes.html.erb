<div class="page container">
  <div class="row">
    <%=
      render(:partial => 'shared/slider', :locals => {:slider => {
          :id => "thanksgiving-2017-slider",
          :images => images_for(%W(seniors thanksgiving-2017)),
          :options => {'pauseTime' => '5000', 'controlNav' => true, 'controlNavThumbs' => true}
      }})
    %>
    <div class="sixcol last">
      <p class="page-title center-text">Hoboken Thanksgiving Dinner</p>

      <p class="page-text">Walk The Walk has been providing home-cooked holiday meals to shut-in and needy senior
        citizens in areas including Howell Township, Hoboken, and New Brunswick.</p>
    </div>
  </div>
  <div class="separator"></div>
  <div class="row">
    <%=
        render(:partial => 'shared/slider', :locals => {:slider => {
                :id => "thanksgiving-2011-slider",
                :images => images_for(%W(seniors thanksgiving-2011)),
                :options => {'pauseTime' => '5000', 'controlNav' => true, 'controlNavThumbs' => true}
        }})
    %>
    <div class="sixcol last">
      <p class="page-title center-text">2011 Hoboken Thanksgiving Dinner</p>

      <p class="page-text"></p>

      <div class="page-section">
        <p class="page-subtitle">Articles</p>
        <%= link_to("Walk the Walk Foundation founders deliver Thanksgiving to Hoboken seniors", 'http://www.nj.com/jjournal-news/index.ssf/2011/11/walk_the_walk_foundation_found.html', :target => '_blank'); %>
        <br/>
        <%= link_to("Walk the Walk foundation makes Thanksgiving meals for shut-in seniors in Hoboken", 'http://www.nj.com/hobokennow/index.ssf/2011/11/walk_the_walk_foundation_makes.html', :target => '_blank'); %>
        <br/>
      </div>
    </div>
  </div>
  <div class="separator"></div>
  <div class="row">
    <%=
        render(:partial => 'shared/slider', :locals => {:slider => {
                :id => "thanksgiving-2010-slider",
