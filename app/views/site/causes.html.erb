                :options => {'pauseTime' => '5000', 'directionNav' => false, 'controlNav' => false, 'controlNavThumbs' => false}
        }})
    %>
    <div class="sixcol last">
      <p class="page-title center-text">2009 Hoboken Thanksgiving Dinner</p>

      <p class="page-text"></p>

      <div class="page-section">
        <p class="page-subtitle">Articles</p>
