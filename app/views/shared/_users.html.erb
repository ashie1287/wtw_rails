<table class="object-table center-table">
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Actions</th>
    <th></th>
    <th></th>
  </tr>

  <% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.email %></td>
      <td>
        <%= button_to 'Show', user_path(user), :method => :get %>
        <%= button_to 'Edit', edit_user_path(user), :method => :get %>
        <%= button_to 'Destroy', user_path(user), :confirm => 'Are you sure?', :method => :delete %>
      </td>
    </tr>
  <% end %>
</table>
